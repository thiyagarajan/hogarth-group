<div id="myTabContent" class="tab-content">
<div class="tab-pane fade in active" id="mon">

<!-- MONDAY EXAMPLE -->
<table cellpadding='0' cellspacing='0' class="table no-border">
<tr>
  <th>TIME</th>
  <th>ACTIVITY</th>
  <th>INSTRUCTOR</th>
  <th class='hidden-xs hidden-sm'>AREA</th>
  <th>LEVEL</th>
  <th class='hidden-xs'>SESSION &pound;</th>
</tr>

<tr class='morning'>
  <td class='time'><div class='label'>Morning</div></td>
  <td colspan='7'></td>
</tr>

{% for activity in timetable.activities %}
{% for day in activity.days %}
  
{% if day.name == 'Monday' %}
  <tr class='adult'>
    <td class='time'>TEST{{ slot_time(2,3) }}TEST {{ activity.starts_at | localized_date: '%H:%M' }} - {{ activity.ends_at | localized_date: '%H:%M' }}</td>
    <td title='50 / 50'><a href="/timetable_activities/9122">{{ activity.name }}</a></td>
    <td title='Tara'><a href="{{ activity.instructor._slug }}">{{ activity.instructor.name }}</a></td>
    <td class='hidden-xs hidden-sm' title='Studio 1'>{{ activity.location }}</td>
    <td title='General'>{{ activity.level }}</td>
    <td class='hidden-xs'>{{ activity.session_price }}</td>
  </tr>
  {% endif %}
  {% endfor %}
{% endfor %}

<tr class='afternoon'>
  <td class='time'><div class='label'>Afternoon</div></td>
  <td colspan='7'></td>
</tr>

<tr class='adult'>
  <td class='time'>07:30 - 08:30</td>
