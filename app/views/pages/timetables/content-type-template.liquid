      <div class="col-sm-7 col-xs-12">
        <select class="selectpicker show-tick">
          <optgroup label="Young members" data-max-options="2">  
            {% for content in contents.timetables %}
            <option value="{{ content._slug }}">{{ content.title }}</option>
            {% endfor %}
          </optgroup>
          <optgroup label="Adults" data-max-options="2">
            {% for content in contents.timetables %}
            <option>{{ content.title }}</option>
            {% endfor %}
          <optgroup>          
        </select>
      </div>

      <div class="col-sm-5 col-xs-12">
        <span class="hhc-pdf"><a href="https://s3.amazonaws.com/the-park-club-website/hogarth_spring_2015.pdf" target="_blank">Download as PDF</a></span>
      </div>

      </div>

<!-- Timetable start -->

<ul id="myTab" class="nav nav-tabs">
  <li class="active"><a href="#mon" data-toggle="tab" class="selected">MON</a></li>
  <li><a href="#tues" data-toggle="tab">TUES</a></li>
  <li><a href="#wed" data-toggle="tab">WED</a></li>
